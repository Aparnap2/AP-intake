{
  "dashboard": {
    "id": null,
    "title": "AP Intake System Overview",
    "tags": ["ap-intake", "overview"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Invoice Processing Volume",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(ap_intake_invoices_processed_total[5m])",
            "legendFormat": "Invoices/second"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            }
          },
          "overrides": []
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Processing Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(ap_intake_invoices_processed_total{status=\"success\"}[5m])) / sum(rate(ap_intake_invoices_processed_total[5m])) * 100",
            "legendFormat": "Success Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 80
                },
                {
                  "color": "green",
                  "value": 95
                }
              ]
            },
            "unit": "percent",
            "max": 100,
            "min": 0
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Average Processing Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(ap_intake_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(ap_intake_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          }
        ],
        "yAxes": [
          {
            "label": "Duration (seconds)"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(ap_intake_invoices_processed_total{status=\"error\"}[5m])",
            "legendFormat": "Errors/second"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Active Workflows",
        "type": "stat",
        "targets": [
          {
            "expr": "ap_intake_active_workflows",
            "legendFormat": "Active Workflows"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Queue Sizes",
        "type": "timeseries",
        "targets": [
          {
            "expr": "ap_intake_queue_size",
            "legendFormat": "{{queue_name}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 6,
          "y": 16
        }
      },
      {
        "id": 7,
        "title": "Database Connections",
        "type": "stat",
        "targets": [
          {
            "expr": "ap_intake_database_connections",
            "legendFormat": "{{pool}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 16
        }
      },
      {
        "id": 8,
        "title": "SLO Achievement - Time to Ready",
        "type": "gauge",
        "targets": [
          {
            "expr": "ap_intake_slo_achieved_percentage{slo_name=\"Time-to-Ready Processing\",sli_type=\"time_to_ready\"}",
            "legendFormat": "SLO Achievement %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "max": 100,
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 80
                },
                {
                  "color": "green",
                  "value": 95
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 9,
        "title": "Extraction Confidence Distribution",
        "type": "heatmap",
        "targets": [
          {
            "expr": "rate(ap_intake_extraction_confidence_bucket[5m])",
            "legendFormat": "{{le}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        }
      },
      {
        "id": 10,
        "title": "API Request Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(ap_intake_api_requests_total[5m])) by (endpoint)",
            "legendFormat": "{{endpoint}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 32
        }
      },
      {
        "id": 11,
        "title": "Resource Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "ap_intake_memory_usage_bytes / 1024 / 1024",
            "legendFormat": "Memory (MB) - {{component}}"
          },
          {
            "expr": "ap_intake_cpu_usage_percent",
            "legendFormat": "CPU % - {{component}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 40
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}